{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit"
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true,
    "**/node_modules": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true,
    "**/node_modules": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
